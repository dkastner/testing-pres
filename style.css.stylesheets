h1, h2, h3 { 
  text-align: center;
}
img { 
  display: block;
  margin: 0 25%;
}
div.highlight, pre {
  margin-bottom: 5%;
}

pre {
  background-color: black;
  color: #EEE;
  padding: 2%;
}

div.highlight pre {
  background-color: white !important;
  color: black;
}

ul, ol { 
  margin: 0 10%;
}

ol li {
  padding: auto;
}
